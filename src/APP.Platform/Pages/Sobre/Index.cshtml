@section Scripts {
<link
  rel="stylesheet"
  href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
/>

<script>
  document.body.style.marginBottom = "0";


  const containerElements = document.querySelectorAll("div.container");
  const secondContainerElement = containerElements[1];
  const divRoxo = document.getElementById("divRoxo");

  secondContainerElement.style.marginRight = "0px";
  secondContainerElement.classList.add("p-0");
  secondContainerElement.classList.add("m-0");

  const theGreatContent = document.getElementById("theGreatContent");
  theGreatContent.style.marginLeft = "0px";
  theGreatContent.setAttribute("style", "width: 100% !important;");

  const firstContainer = document.getElementById("firstContainer")

  function updateMarginLeft() {
    if (window.innerWidth > 992) {
      firstContainer.classList.remove("mx-4")
      divRoxo.classList.remove("mx-4")
      firstContainer.style.marginLeft="30px"
    } else {
      document.body.style.marginLeft= "0px";
      secondContainerElement.style.paddingLeft = "0";
      firstContainer.classList.add("mx-4")
      divRoxo.classList.add("mx-4")
    }
  }

  updateMarginLeft();
  window.addEventListener("resize", updateMarginLeft);
</script>
}
